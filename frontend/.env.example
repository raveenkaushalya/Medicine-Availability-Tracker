# Example environment variables for frontend

# API Configuration
VITE_API_URL=http://localhost:3000
VITE_API_VERSION=v1

# Google Maps API (if using maps feature)
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Feature Flags
VITE_ENABLE_MAPS=true
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_ANALYTICS=false

# Analytics (optional)
VITE_GOOGLE_ANALYTICS_ID=
VITE_SENTRY_DSN=

# App Configuration
VITE_APP_NAME=Medicine Availability Tracker
VITE_APP_VERSION=1.0.0

# Default Settings
VITE_DEFAULT_LOCATION_LAT=6.9271
VITE_DEFAULT_LOCATION_LNG=79.8612
VITE_SEARCH_RADIUS_KM=10
